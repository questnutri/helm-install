meta {
  name: Get All Diets from Patient
  type: http
  seq: 1
}

get {
  url: http://{{GATEWAY_URL}}/diet
  body: json
  auth: inherit
}

body:json {
  {
    "patientId": "{{patientId}}",
    "nutritionistId": "{{nutritionistId}}"
  }
}

vars:pre-request {
  patientId: {{PATIENT_ID}}
  nutritionistId: {{NUTRITIONIST_ID}}
}

settings {
  encodeUrl: true
}
