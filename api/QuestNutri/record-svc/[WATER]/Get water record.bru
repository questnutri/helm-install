meta {
  name: Get water record
  type: http
  seq: 2
}

get {
  url: {{HTTP_PROTOCOL}}://{{GATEWAY_URL}}/record/water/patient/:patientId/all
  body: json
  auth: bearer
}

params:query {
  ~date: 2025-12-03
  ~includeRegisters: false
}

params:path {
  patientId: {{PATIENT_ID}}
}

auth:bearer {
  token: {{PATIENT_ACCESS_TOKEN}}
}

settings {
  encodeUrl: true
  timeout: 0
}
