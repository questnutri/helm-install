meta {
  name: Create water record
  type: http
  seq: 1
}

post {
  url: {{HTTP_PROTOCOL}}://{{GATEWAY_URL}}/record/water
  body: json
  auth: bearer
}

auth:bearer {
  token: {{PATIENT_ACCESS_TOKEN}}
}

body:json {
  {
    "amountInMl": "200",
    "operation": "ADD"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
