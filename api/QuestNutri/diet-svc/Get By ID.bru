meta {
  name: Get By ID
  type: http
  seq: 2
}

get {
  url: {{HTTP_PROTOCOL}}://{{GATEWAY_URL}}/diet/:createdDiet
  body: none
  auth: bearer
}

params:path {
  createdDiet: {{dietId}}
}

auth:bearer {
  token: {{NUTRITIONIST_ACCESS_TOKEN}}
}

vars:pre-request {
  dietId: {{createdDiet}}
}

settings {
  encodeUrl: true
  timeout: 0
}
