meta {
  name: [P] Current plan
  type: http
  seq: 2
}

get {
  url: {{HTTP_PROTOCOL}}://{{GATEWAY_URL}}/diet/current/plan?includeRecords=true&monthlyView=true
  body: json
  auth: bearer
}

params:query {
  includeRecords: true
  monthlyView: true
  ~date: 2025-11-01
  ~length: 24
}

auth:bearer {
  token: {{PATIENT_ACCESS_TOKEN}}
}

body:json {
  {
    "patientId": "{{PATIENT_ID}}",
    "nutritionistId": "{{NUTRITIONIST_ID}}"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
