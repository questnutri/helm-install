meta {
  name: Get All Diets from Patient
  type: http
  seq: 1
}

post {
  url: {{HTTP_PROTOCOL}}://{{GATEWAY_URL}}/diet
  body: json
  auth: bearer
}

auth:bearer {
  token: {{PATIENT_ACCESS_TOKEN}}
}

body:json {
  {
    "patientId": "{{patientId}}",
    "nutritionistId": "{{nutritionistId}}"
  }
}

vars:pre-request {
  patientId: {{PATIENT_ID}}
  nutritionistId: {{NUTRITIONIST_ID}}
}

settings {
  encodeUrl: true
  timeout: 0
}
