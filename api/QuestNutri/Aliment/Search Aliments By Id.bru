meta {
  name: Search Aliments By Id
  type: graphql
  seq: 2
}

post {
  url: http://{{GATEWAY_URL}}/aliment/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
  findManyAlimentById(ids:["68ec1b03acda5e8f15d5c3e8"]) {
        _id
        source
        name
        portions
    }
  }
}

settings {
  encodeUrl: true
}
