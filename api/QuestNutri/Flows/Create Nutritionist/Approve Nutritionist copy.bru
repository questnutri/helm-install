meta {
  name: Approve Nutritionist copy
  type: http
  seq: 3
}

post {
  url: http://{{GATEWAY_URL}}/admin/approve-nutritionist
  body: json
  auth: bearer
}

auth:bearer {
  token: {{ADMIN_ACCESS_TOKEN}}
}

body:json {
  {
      "email": "{{NUTRITIONIST_EMAIL}}"
  }
}

script:post-response {
  if(res.status === 201) {
    bru.setVar("NUTRITIONIST_ID",res("id"));
  }
}

settings {
  encodeUrl: true
}
