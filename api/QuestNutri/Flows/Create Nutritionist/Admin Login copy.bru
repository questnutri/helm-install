meta {
  name: Admin Login copy
  type: http
  seq: 2
}

post {
  url: {{HTTP_PROTOCOL}}://{{GATEWAY_URL}}/admin/login
  body: json
  auth: none
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{password}}"
  }
}

vars:pre-request {
  email: {{ADMIN_EMAIL}}
  password: 
}

vars:post-response {
  ADMIN_ACCESS_TOKEN: res("accessToken")
  ADMIN_REFRESH_TOKEN: res("refreshToken")
}

settings {
  encodeUrl: true
  timeout: 0
}
