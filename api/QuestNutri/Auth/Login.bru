meta {
  name: Login
  type: http
  seq: 1
}

post {
  url: {{HTTP_PROTOCOL}}://{{GATEWAY_URL}}/auth/login
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{password}}"
  }
}

vars:pre-request {
  email: test@admin.com
  password: test
}

vars:post-response {
  ACCESS_TOKEN: res("accessToken")
  REFRESH_TOKEN: res("refreshToken")
}

settings {
  encodeUrl: true
}
